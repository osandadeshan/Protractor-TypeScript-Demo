    "webdriver-start": "webdriver-manager start",
    "test": "protractor compiled_typescript/config.js"
  },
  "dependencies": {
    "@types/jasmine": "2.5.41",
    "@types/jasminewd2": "^2.0.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/selenium-webdriver": "^3.0.14",
    "jasmine": "^2.4.1",
    "protractor": "^5.4.2",
    "typescript": "^2.0.10",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "@types/jasminewd2": "^2.0.0",
    "protractor-beautiful-reporter": "^1.2.7",
    "ts-node": "^3.3.0"
  }
