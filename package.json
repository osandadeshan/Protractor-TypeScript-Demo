  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "pretest": "npm run tsc",
    "debug": "node --inspect-brk ./node_modules/.bin/protractor asyncAwait/conf.js",
    "build": "tsc",
    "clean": "rimraf compiled_typescript/",
    "clean-build": "npm run clean && npm run build",
    "init-contributors": "all-contributors init",
    "add-contributors": "all-contributors add",
